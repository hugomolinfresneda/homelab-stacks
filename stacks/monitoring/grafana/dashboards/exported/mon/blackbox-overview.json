      },
      {
        "name": "target",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(probe_success{job=~\"blackbox-http.*|blackbox-icmp\"}, instance)",
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "selected": true,
          "text": [
            "All"
          ],
          "value": [
            "$__all"
          ]
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Availability (probe_success)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "max by (instance) (probe_success{job=~\"blackbox-http.*|blackbox-icmp\", instance=~\"$target\"})",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto"
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Latency (avg 5m)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "avg by (instance) (avg_over_time(probe_duration_seconds{job=~\"blackbox-http.*\", instance=~\"$target\"}[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "table",
      "title": "HTTP Status Code (last)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "max by (instance) (probe_http_status_code{job=~\"blackbox-http.*\", instance=~\"$target\"})",
          "refId": "A"
        }
      ],
      "options": {
        "showHeader": true
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "TLS Expiry (days)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "(probe_ssl_earliest_cert_expiry{job=~\"blackbox-http.*\", instance=~\"$target\"} - time()) / 86400",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      }
    }
  ]
}