# Nextcloud DB secrets (example)
# Copy to your runtime stack as db.env and fill in real values.
NC_DB_NAME=exampledb
NC_DB_USER=exampleuser
NC_DB_PASS=changeme
NC_DB_ROOT=changeme

# Keep in sync with NC_* values
MYSQL_DATABASE=${NC_DB_NAME}
MYSQL_USER=${NC_DB_USER}
MYSQL_PASSWORD=${NC_DB_PASS}
MYSQL_HOST=your-db-host
